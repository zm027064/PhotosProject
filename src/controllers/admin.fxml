<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminController" spacing="8">
  <padding>
    <Insets top="10" right="10" bottom="10" left="10" />
  </padding>
  <Label text="Admin - Manage Users" />
  <ListView fx:id="userListView" prefHeight="300" />
  <HBox spacing="8">
    <Button fx:id="createButton" text="Create User" onAction="#handleCreate" />
    <Button fx:id="deleteButton" text="Delete User" onAction="#handleDelete" />
    <Button fx:id="backButton" text="Back" onAction="#handleBack" />
  </HBox>
</VBox>
